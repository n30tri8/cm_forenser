nav.navbar.navbar-expand-md.navbar-dark.fixed-top.bg-dark
  a.navbar-brand(href='/') Copy-Move Forenser
  button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarCollapse' aria-controls='navbarCollapse' aria-expanded='false' aria-label='Toggle navigation')
    span.navbar-toggler-icon
  #navbarCollapse.collapse.navbar-collapse
    ul.navbar-nav.mr-auto
      - if (typeof session != 'undefined' && typeof session.user != 'undefined')
        li.nav-item(class='active')
          a.nav-link(href="#", title="Profile")= session.user.username
        - if (typeof isAdminUser != 'undefined' && isAdminUser)
          li.nav-item(class='active')
            a.nav-link(href="/users", title="Admin Panel") Admin Panel
        li.nav-item
          a.nav-link(href="/logout", title="logout") Logout
      - else
        li(class='active nav-item')
          a.nav-link(href="/login", title="Login") Login